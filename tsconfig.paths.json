{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@root/*": [
        "src/*"
      ],
      "@infra/*": [
        "src/infra/*"
      ],
      "@interfaces/*": [
        "src/interfaces/*"
      ],
      "@definitions/*": [
        "src/definitions/*"
      ],
      "@literals/*": [
        "src/literals/*"
      ],
      "@helpers/*": [
        "src/helpers/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@catalogs/*": [
        "src/catalogs/*"
      ],
      "@errors/*": [
        "src/errors/*"
      ],
      "@dtos/*": [
        "src/dtos/*"
      ],
      "@entities/*": [
        "src/entities/*"
      ],
      "@messages/*": [
        "src/messages/*"
      ],
      "@rules/*": [
        "src/rules/*"
      ],
      "@signals/*": [
        "src/signals/*"
      ],
      "@filters/*": [
        "src/filters/*"
      ]
    }
  }
}